"use strict";

var _resistorColor = require("./resistor-color");

describe('ResistorColor', () => {
  describe('Color codes', () => {
    test('Black', () => {
      expect((0, _resistorColor.colorCode)("black")).toEqual(0);
    });
    test('White', () => {
      expect((0, _resistorColor.colorCode)("white")).toEqual(9);
    });
    test('Orange', () => {
      expect((0, _resistorColor.colorCode)("orange")).toEqual(3);
    });
  });
  test('Colors', () => {
    expect(_resistorColor.COLORS).toEqual(["black", "brown", "red", "orange", "yellow", "green", "blue", "violet", "grey", "white"]);
  });
});
