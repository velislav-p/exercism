"use strict";

var _twoFer = require("./two-fer");

describe('twoFer()', () => {
  test('no name given', () => {
    expect((0, _twoFer.twoFer)()).toEqual("One for you, one for me.");
  });
  test('a name given', () => {
    expect((0, _twoFer.twoFer)("Alice")).toEqual("One for Alice, one for me.");
  });
  test('another name given', () => {
    expect((0, _twoFer.twoFer)("Bob")).toEqual("One for Bob, one for me.");
  });
});
