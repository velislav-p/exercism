"use strict";

var _leap = require("./leap");

describe('A leap year', () => {
  test('year not divisible by 4: common year', () => {
    expect((0, _leap.isLeap)(2015)).toBe(false);
  });
  test('year divisible by 4, not divisible by 100: leap year', () => {
    expect((0, _leap.isLeap)(2016)).toBe(true);
  });
  test('year divisible by 100, not divisible by 400: common year', () => {
    expect((0, _leap.isLeap)(2100)).toBe(false);
  });
  test('year divisible by 400: leap year', () => {
    expect((0, _leap.isLeap)(2000)).toBe(true);
  });
  test('year divisible by 200, not divisible by 400: common year', () => {
    expect((0, _leap.isLeap)(1800)).toBe(false);
  });
});
