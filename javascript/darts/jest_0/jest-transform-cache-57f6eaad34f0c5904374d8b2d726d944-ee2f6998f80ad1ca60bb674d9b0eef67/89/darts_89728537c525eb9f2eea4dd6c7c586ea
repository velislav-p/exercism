const solve = (x, y) => {
  const h = Math.sqrt(Math.pow(x, 2) + Math.pow(y, 2));

  if (h > 5 && h <= 10) {
    return 1;
  }

  if (h > 1 && h <= 5) {
    return 5;
  }

  if (h <= 1) {
    return 10;
  } else {
    return 0;
  }
};

exports.solve = solve;
